<preset name="Тест" name:en="Test" lang="ru">
  <button label="Магазин" label:en="Shop" icon="shop_convenience" directional="yes">
    <poi type="shop_convenience" />
    <properties>
      <property name="Название" name:en="Title" k="title" />
    </properties>
  </button>
  <button label="Освещение" label:en="Lighting" toggle="yes">
    <poi type="lit" />
    <properties>
      <property name="У дороги" name:en="Near road" k="near_road" type="boolean" />
    </properties>
  </button>
  
  <button label="Зебра" label:en="Zebra cross">
  	<poi type="zebra"/>
  </button>
  
  <button label="Мост" label:en="Bridge" toggle="yes">
  	<poi type="bridge"/>
  </button>
  
  <button label="Населенный пункт" label:en="City limit">
    <poi type="city_limit" />
    <properties>
      <property name="фон" name:en="Sign background" k="colour:back" type="choice">
        <variant name="белый" name:en="white" v="white" />
        <variant name="синий" name:en="blue" v="blue" />
      </property>
    </properties>
  </button>
  
  <button label="Км. столб" label:en="Milestone" icon="highway_milestone">
    <poi type="milestone" />
    <properties>
      <property name="км" name:en="distance" k="pk" type="sequence" />
      <property name="обр.км" name:en="reverse distance" k="pk:back" type="sequence" />
    </properties>
  </button>
</preset>